    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/utilisateur/src/**/*.ts",
          "libs/utilisateur/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
